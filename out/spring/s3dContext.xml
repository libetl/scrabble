<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="autodetect">

	<bean id="s3dHelper" class="org.toilelibre.libe.scrabble.s3d.impl.S3DHelper">
		<property name="implPackage">
			<value>org.toilelibre.libe.scrabble.j3d</value>
		</property>
		<property name="implPrefix">
			<value>J3D</value>
		</property>
	</bean>

	<bean id="canvas3d" class="java.lang.String">
		<constructor-arg>
			<value>org.toilelibre.libe.scrabble.j3d.model.J3DCanvas3D</value>
		</constructor-arg>
	</bean>

	<bean id="s3dLoader" class="org.toilelibre.libe.scrabble.j3d.loader.J3DLoader">
	</bean>

</beans>
